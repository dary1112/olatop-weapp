page {
  box-sizing: border-box;
  min-height: 100vh;
  color: #323233;
  font-size: 16px;
  font-family: -apple-system, BlinkMacSystemFont, 'Helvetica Neue', Helvetica,
    Segoe UI, Arial, Roboto, 'PingFang SC', 'miui', 'Hiragino Sans GB', 'Microsoft Yahei',
    sans-serif;
  background: #f7f8fa;
  -webkit-font-smoothing: antialiased;
}

.demo-margin-left {
  margin-left: 10px;
}

.demo-margin-right {
  margin-right: 10px;
}

.demo-margin-bottom {
  display: block;
  margin-bottom: 15px;
}

.charts-wrap {
  display: flex;
  flex-direction: column;
  align-items: center;
  min-height: 96vh;
  padding-bottom: 4vh;
}

.container {
  width: 700rpx;
  height: 700rpx;
  margin: 0 auto;
}
.options {
  flex: 1;
}
